{
  "name": "kuocli",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "audit:prod": "yarn-audit-fix --only=prod",
    "format:all": "nx format:write --all",
    "lint:all": "nx run-many --all --target=lint --fix",
    "test:all": "nx run-many --all --target=test --codeCoverage",
    "predeploy:commons": "dotenv -- cross-var npm unpublish --force @kuocli/commons --registry=%NPM_REGISTRY% || 2>/dev/null",
    "deploy:commons": "nx build commons && dotenv -- cross-var npm publish dist/packages/commons/ --tag=latest --registry=%NPM_REGISTRY%",
    "update:nx": "nx migrate latest && yarn && (test -f migrations.json && (nx migrate --run-migrations && rm migrations.json) || 2>/dev/null)",
    "prepare": "husky install"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "~17.6.1",
    "@commitlint/config-conventional": "~17.6.1",
    "@commitlint/cz-commitlint": "~17.5.0",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/jest": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/nx-plugin": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/jest": "29.4.4",
    "@types/lodash": "~4.14.191",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "commitizen": "~4.3.0",
    "cross-var": "1.1.0",
    "dotenv-cli": "7.2.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "git-branch-is": "~4.0.0",
    "husky": "~8.0.3",
    "inquirer": "8.2.5",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "~13.2.0",
    "nx": "15.9.2",
    "prettier": "~2.8.5",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yarn-audit-fix": "~9.3.10"
  },
  "dependencies": {
    "@nrwl/devkit": "15.9.2",
    "@swc/helpers": "~0.4.11",
    "lodash": "~4.17.21",
    "tslib": "^2.3.0"
  },
  "resolutions": {
    "json5": ">=1.0.2"
  }
}
