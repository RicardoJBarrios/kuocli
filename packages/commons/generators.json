{
  "$schema": "http://json-schema.org/schema",
  "name": "commons",
  "version": "0.0.1",
  "generators": {
    "gitlint": {
      "factory": "./src/generators/gitlint/generator",
      "schema": "./src/generators/gitlint/schema.json",
      "description": "Adhere the workspace to a git commit convention."
    },
    "codelint": {
      "factory": "./src/generators/codelint/generator",
      "schema": "./src/generators/codelint/schema.json",
      "description": "Static check on source code to detect stylistic or programmatic errors."
    }
  }
}
